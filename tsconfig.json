{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs" /* Specify module code generation. */,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true /* Enable all strict type-checking options. */,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true
  }
}
